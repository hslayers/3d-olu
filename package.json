{
  "name": "3d-olu",
  "private": false,
  "version": "0.1.1",
  "description": "This application visualises the http://sdi4apps.eu/open_land_use/ Open Land Use dataset on top of the EU-DEM terrain model in a perspective view. Displaying the data in 3D environment helps the user to explore an area of interest in more natural way than a traditional map. Moreover, the user can explore other datasets (such as http://opentransportmap.info/ OTM or http://sdi4apps.eu/spoi/ SPOI) or even add a web map service of his choice (as long as the WMS is in WGS84 coordinate system) to make custom 3D mash-ups. The technical details can be found http://www.plan4all.eu/2017/08/3d-open-land-use-team/",
  "repository": "git+ssh://git@github.com:hslayers/project-rostenice.git",
  "license": "MIT",
  "scripts": {
    "postinstall": "",
    "prestart": "npm install",
    "start": "http-server -a localhost -p 8000",
    "pretest": "npm install",
    "test": "karma start test/karma.conf.js",
    "test-single-run": "karma start test/karma.conf.js  --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor test/protractor-conf.js"
  },
  "dependencies": {
    "cesium": "1.61.0",
    "hslayers-ng": "^1.8.0",
    "jquery": "^3.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "angular-jsdoc": "^1.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-angularjs-annotate": "^0.10.0",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "extract-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "grunt": "^1.5.2",
    "grunt-angular-gettext": "^2.5.0",
    "grunt-contrib-uglify": "^3.3.0",
    "grunt-contrib-uglify-es": "git://github.com/gruntjs/grunt-contrib-uglify.git#harmony",
    "grunt-jsbeautifier": "0.2.x",
    "grunt-jsdoc": "^2.4.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "ng-cache-loader": "0.0.26",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-prefix-selector": "^1.7.2",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.41.2",
    "webpack-build-notifier": "^1.1.1",
    "webpack-cli": "^3.3.9",
    "webpack-merge": "^4.2.2"
  }
}
